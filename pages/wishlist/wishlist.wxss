.container {
  padding: 20px;
  background-color: #f5f5f5;
}

.header {
  text-align: center;
  margin-bottom: 20px;
}

.title {
  font-size: 24px;
  color: #333;
}

.wishlist-section {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.weui-media-box {
  width: 48%;
  margin-bottom: 20px;
}

.listing-image {
  width: 100%;
  height: 150px;
  border-radius: 10px;
}

.listing-tags {
  display: flex;
  flex-wrap: wrap;
}

.tag {
  background-color: #ffcc00;
  color: #fff;
  padding: 5px 10px;
  border-radius: 20px;
  font-size: 12px;
  margin-right: 10px;
  margin-bottom: 10px;
}

.listing-description {
  font-size: 14px;
  color: #666;
}